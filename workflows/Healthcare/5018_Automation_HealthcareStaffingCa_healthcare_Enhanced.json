    "business_metadata": {
      "useCase": "Segment a database of nursing candidates into actionable groups for targeted recruitment. Automatically calculates experience level (New Grad, Intermediate, Experienced, Senior), specialty segments (ICU, ER, Med-Surg, Peds, Other), location preferences, shift preferences, pay tiers (Standard, Premium, Executive), and engagement levels (Hot, Warm, Cold). Alerts recruiters via Slack for hot leads requiring immediate action.",
      "difficulty": "intermediate",
      "estimatedSetupTime": "25-35 minutes",
      "features": [
        "Multi-dimensional segmentation (6 criteria)",
        "Specialty routing (ICU, ER, Med-Surg, Pediatrics, Other)",
        "Experience level classification (4 tiers)",
        "Engagement scoring (Hot/Warm/Cold)",
        "Pay tier assignment (Standard/Premium/Executive)",
        "Market rate ranges by specialty",
        "Recruiting priority levels",
        "Automated hot lead alerts to Slack",
        "SLA recommendations per segment",
        "Recommended actions per engagement level"
      ],
      "limitations": [
        "Requires clean candidate data",
        "Manual specialty mapping needed",
        "No machine learning predictions",
        "Static segmentation rules",
        "Requires engagement tracking setup"
      ],
      "requirements": [
        "Google Sheets with 'Candidates' sheet",
        "Slack workspace and channel",
        "Candidate data: specialty, years_experience, location, shift preference, desired pay, engagement metrics"
      ],
      "tags": [
        "candidate-segmentation",
        "lead-scoring",
        "healthcare-staffing",
        "recruitment-automation",
        "nurse-recruitment",
        "data-enrichment",
        "lead-routing"
      ],
      "integrations": [
        "Google Sheets",
        "Slack"
      ],
      "pricing": {
        "n8n": "Free (self-hosted) or $20+/month (cloud)",
        "googleSheets": "Free",
        "slack": "Free tier available"
      },
      "lastUpdated": "2025-11-19"
    }
  },
  "nodes": [
