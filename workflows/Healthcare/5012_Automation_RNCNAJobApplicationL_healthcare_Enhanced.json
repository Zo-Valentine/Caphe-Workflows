    "business_metadata": {
      "useCase": "Healthcare staffing agencies need to capture RN and CNA job applications from their website forms and immediately notify recruiters. This workflow automates the entire lead capture process - from form submission to database storage to multi-channel notifications.",
      "difficulty": "beginner",
      "estimatedSetupTime": "20-30 minutes",
      "features": [
        "Webhook endpoint for form submissions",
        "Automatic data extraction from POST body",
        "Real-time Google Sheets database storage",
        "HTML-formatted email notifications to recruiters",
        "Slack channel notifications with candidate details",
        "Immediate webhook response to applicant",
        "Timestamp tracking for all applications",
        "Automatic status assignment (New Lead)"
      ],
      "limitations": [
        "Requires Google Sheets for data storage (not CRM-integrated)",
        "No automatic email confirmation to applicant",
        "No duplicate detection (same applicant can submit multiple times)",
        "No file upload handling (resume attachments)"
      ],
      "requirements": {
        "credentials": [
          "Google Sheets OAuth2 (read/write access)",
          "SMTP Email Account (for recruiter notifications)",
          "Slack API Token (for channel notifications)"
        ],
        "external": [
          "Google Sheet with 'Candidates' tab and columns: Name, Email, Phone, Position, License Number, License State, Years Experience, Specialty, Preferred Location, Shift Preference, Application Date, Status",
          "Slack channel for recruitment notifications",
          "Web form that POSTs to n8n webhook URL"
        ],
        "environment": [
          "GOOGLE_SHEET_ID - Your Google Sheet ID",
          "RECRUITER_EMAIL - Recruiter's email address",
          "SLACK_CHANNEL - Slack channel ID (e.g., #recruiters)",
          "SHEET_URL - Direct URL to your Google Sheet"
        ]
      },
      "tags": [
        "healthcare",
        "staffing",
        "lead-generation",
        "RN",
        "CNA",
        "recruitment",
        "form-automation",
        "webhooks",
        "notifications"
      ],
      "integrations": [
        "Webhook",
        "Google Sheets",
        "Email (SMTP)",
        "Slack"
      ],
      "pricing": {
        "n8n": "Free tier: 20 executions/day, Paid: $20/month unlimited",
        "googleSheets": "Free with Google account",
        "slack": "Free tier available",
        "smtp": "Varies by provider (Gmail free for basic use)"
      },
      "lastUpdated": "2025-11-19"
    }
  },
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "rn-cna-application",
        "responseMode": "responseNode",
        "options": {
          "allowedOrigins": "*"
        }
      },
      "id": "webhook-trigger",
      "name": "Webhook - Job Application Form",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        250,
        300
      ],
      "webhookId": "rn-cna-application-form"
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "applicant_name",
              "value": "={{ $json.body.full_name }}"
            },
            {
              "name": "email",
              "value": "={{ $json.body.email }}"
            },
            {
              "name": "phone",
              "value": "={{ $json.body.phone }}"
            },
            {
              "name": "position",
              "value": "={{ $json.body.position }}"
            },
            {
              "name": "license_number",
              "value": "={{ $json.body.license_number }}"
            },
            {
              "name": "license_state",
              "value": "={{ $json.body.license_state }}"
            },
            {
              "name": "years_experience",
              "value": "={{ $json.body.years_experience }}"
            },
            {
              "name": "specialty",
              "value": "={{ $json.body.specialty }}"
            },
            {
              "name": "preferred_location",
              "value": "={{ $json.body.preferred_location }}"
            },
            {
              "name": "shift_preference",
              "value": "={{ $json.body.shift_preference }}"
            },
            {
              "name": "application_date",
              "value": "={{ $now.toISO() }}"
            },
            {
              "name": "status",
              "value": "New Lead"
            }
          ]
        },
        "options": {}
      },
      "id": "extract-data",
      "name": "Extract Application Data",
      "type": "n8n-nodes-base.set",
      "typeVersion": 2,
      "position": [
        450,
        300
      ]
    },
    {
      "parameters": {
        "operation": "append",
        "sheetId": "={{ $env.GOOGLE_SHEET_ID }}",
        "range": "Candidates!A:L",
        "options": {
          "valueInputMode": "USER_ENTERED"
        }
      },
      "id": "save-to-sheet",
      "name": "Save to Google Sheets",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4,
      "position": [
        650,
        300
      ],
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "google-sheets-oauth",
          "name": "Google Sheets Account"
        }
      }
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "notification-message",
              "name": "notification_message",
